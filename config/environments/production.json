{
  "port": 3000,
  "cors": {
    "enabled": true,
    "origins": ["https://chatgpt.com", "https://claude.ai"],
    "credentials": true
  },
  "rateLimit": {
    "windowMs": 900000,
    "max": 1000,
    "aiOptimized": true
  },
  "logging": {
    "level": "info",
    "format": "json"
  },
  "firebase": {
    "enabled": false,
    "gracefulDegradation": true
  },
  "security": {
    "helmet": true,
    "authRequired": true
  }
}